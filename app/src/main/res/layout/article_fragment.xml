<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    android:orientation="vertical">

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="wrap_content"
        android:layout_height="fill_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true">

        <LinearLayout
            android:id="@+id/layoutThree"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_below="@+id/layoutTwo"
            android:layout_marginTop="10dp"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="100">

                <android.support.v7.widget.CardView
                    android:id="@+id/card_view1"
                    android:layout_width="0dp"
                    android:layout_height="110dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:layout_weight="33.3"
                    android:background="@drawable/border_rectangle"
                    card_view:contentPadding="0dp">

                    <RelativeLayout
                        android:id="@+id/rl_cartview1"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="?android:attr/selectableItemBackground">

                        <ImageView
                            android:id="@+id/ivHealthToday"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="20dp"
                            android:background="@mipmap/ic_health5" />

                        <TextView
                            android:id="@+id/tvProtocolEmergencies"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/ivHealthToday"
                            android:layout_centerHorizontal="true"
                            android:text="@string/healthtoday"
                            android:textSize="12sp" />


                    </RelativeLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:id="@+id/card_view2"
                    android:layout_width="0dp"
                    android:layout_height="110dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:layout_weight="33.3"
                    android:background="@drawable/border_rectangle"
                    card_view:contentPadding="0dp">

                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="?android:attr/selectableItemBackground"
                        android:clickable="true"
                        android:focusable="true">

                        <ImageView
                            android:id="@+id/ivMythBreaker"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="20dp"
                            android:background="@mipmap/ic_myth_breaker5" />

                        <TextView
                            android:id="@+id/tvMythBreak"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/ivMythBreaker"
                            android:layout_centerHorizontal="true"
                            android:text="@string/myth"
                            android:textSize="12sp" />


                    </RelativeLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:id="@+id/card_view6"
                    android:layout_width="0dp"
                    android:layout_height="110dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:layout_weight="33.3"
                    android:background="@drawable/border_rectangle"
                    card_view:contentPadding="0dp">

                    <RelativeLayout
                        android:id="@+id/rlPatientAwareness"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="?android:attr/selectableItemBackground"
                        android:clickable="true"
                        android:focusable="true">

                        <ImageView
                            android:id="@+id/ivPatientAwareness"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="20dp"
                            android:background="@mipmap/ic_serious5" />

                        <TextView
                            android:id="@+id/tvPatientAwareness"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/ivPatientAwareness"
                            android:layout_centerHorizontal="true"
                            android:text="@string/serious"
                            android:textAlignment="center"
                            android:textSize="12sp" />

                    </RelativeLayout>
                </android.support.v7.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="100">

                <android.support.v7.widget.CardView
                    android:id="@+id/card_view4"
                    android:layout_width="0dp"
                    android:layout_height="110dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:layout_weight="33.3"
                    android:background="@drawable/border_rectangle"
                    card_view:contentPadding="0dp">

                    <RelativeLayout
                        android:id="@+id/rlHealth2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="?android:attr/selectableItemBackground"
                        android:clickable="true"
                        android:focusable="true">

                        <ImageView
                            android:id="@+id/ivProtocolEmergency"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="20dp"
                            android:background="@mipmap/ic_emergency5" />

                        <TextView
                            android:id="@+id/tvHealthToday"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/ivProtocolEmergency"
                            android:layout_centerHorizontal="true"
                            android:text="@string/protocol"
                            android:textAlignment="center"
                            android:textSize="12sp" />


                    </RelativeLayout>
                </android.support.v7.widget.CardView>

                <android.support.v7.widget.CardView
                    android:id="@+id/card_view5"
                    android:layout_width="0dp"
                    android:layout_height="110dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:layout_weight="33.3"
                    android:background="@drawable/border_rectangle"
                    card_view:contentPadding="0dp">

                    <RelativeLayout
                        android:id="@+id/rlMythBreaker"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="?android:attr/selectableItemBackground"
                        android:clickable="true"
                        android:focusable="true">

                        <ImageView
                            android:id="@+id/ivstorybypharm"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="20dp"
                            android:background="@mipmap/ic_story5" />

                        <TextView
                            android:id="@+id/tvStrorybyPharm"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/ivstorybypharm"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:text="@string/story"
                            android:textAlignment="center"
                            android:textSize="12sp" />


                    </RelativeLayout>
                </android.support.v7.widget.CardView>
                <android.support.v7.widget.CardView
                    android:id="@+id/card_view3"
                    android:layout_width="0dp"
                    android:layout_height="110dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:layout_weight="33.3"
                    android:background="@drawable/border_rectangle"
                    card_view:contentPadding="0dp">

                    <RelativeLayout
                        android:id="@+id/rlBookTest"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="?android:attr/selectableItemBackground"
                        android:clickable="true"
                        android:focusable="true">

                        <ImageView
                            android:id="@+id/ivBookTest"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="20dp"
                            android:background="@mipmap/ic_patitent5" />

                        <TextView
                            android:id="@+id/tvBookTest"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/ivBookTest"
                            android:layout_centerHorizontal="true"
                            android:layout_marginLeft="10dp"
                            android:layout_marginRight="10dp"
                            android:text="@string/patient"
                            android:textAlignment="center"
                            android:textSize="12sp" />

                    </RelativeLayout>
                </android.support.v7.widget.CardView>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:weightSum="100">

                <android.support.v7.widget.CardView
                    android:id="@+id/card_view7"
                    android:layout_width="0dp"
                    android:layout_height="110dp"
                    android:layout_gravity="center"
                    android:layout_margin="5dp"
                    android:layout_weight="33.3"
                    android:background="@drawable/border_rectangle"
                    card_view:contentPadding="0dp">

                    <RelativeLayout
                        android:id="@+id/rlWoman"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="?android:attr/selectableItemBackground"
                        android:clickable="true"
                        android:focusable="true">

                        <ImageView
                            android:id="@+id/ivWoman"
                            android:layout_width="30dp"
                            android:layout_height="30dp"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="20dp"
                            android:background="@mipmap/ic_women_health5" />

                        <TextView
                            android:id="@+id/tvWoman"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/ivWoman"
                            android:layout_centerHorizontal="true"
                            android:text="@string/woman"
                            android:textAlignment="center"
                            android:textSize="12sp" />


                    </RelativeLayout>
                </android.support.v7.widget.CardView>

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="66.7" />
            </LinearLayout>

        </LinearLayout>


    </ScrollView>

</RelativeLayout>